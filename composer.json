    "type": "project",
    "support": {
        "forum": "https://community.openenergymonitor.org/"
    },
    "require": {
        "gettext/gettext": "^4.8.2",
        "gettext/languages": "^2.6.0"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.2.0"
    },
    "scripts": {
        "test": [
            "parallel-lint . --exclude vendor"
        ]
    },
    "license": "AGPL-3.0-or-later",
    "require": {
        "ext-mysqli": "*",
        "ext-gettext": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-ctype": "*"
    }
}
